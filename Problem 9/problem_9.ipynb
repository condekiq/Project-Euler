    "# https://projecteuler.net/problem=9\n",
    "\n",
    "import math\n",
    "\n",
    "epsilon = 0.0001\n",
    "a_max = 500\n",
    "\n",
    "for a in range(1, a_max):\n",
    "    for b in range(a+1, a_max):\n",
    "        c = math.sqrt(a*a + b*b)\n",
    "        if b<c:\n",
    "            if abs(a+b+c - 1000)<epsilon:\n",
    "                ans = a*b*int(c)\n",
    "            \n",
    "ans"
