   "source": [
    "# https://projecteuler.net/problem=2\n",
    "\n",
    "fib0 = 1\n",
    "fib1 = 1\n",
    "sum_fib_even = 0\n",
    "\n",
    "while fib0+fib1 < 4e6:\n",
    "    if (fib0+fib1)%2==0:\n",
    "        sum_fib_even += fib0+fib1\n",
    "    aux = fib0\n",
    "    fib0 = fib1\n",
    "    fib1 += aux\n",
    "    \n",
    "sum_fib_even"
   ]
  }
